parameters:
    level: max
    paths:
        - Endpoint
        - Model
        - Factory
        - Client.php
        - ClientBuilder.php
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        -
            message: '#arrayPropItemTypeMap\(\) is unused.$#'
            paths:
                - ./Model/*
                - ./Endpoint/*
        -
            message: '#maxValues\(\) is unused.$#'
            path: ./Model/*
services:
    -
        class: ADS\JsonImmutableObjects\PhpStan\PropertiesExtension
        tags:
            - phpstan.properties.readWriteExtension